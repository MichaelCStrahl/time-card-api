{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"javascript": {
		"parser": {
      "unsafeParameterDecoratorsEnabled": true
    }
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": "off"
			},
			"complexity": {"noUselessConstructor": "off"}
		}
	}
}